@model IEnumerable<Lab3.Models.PersonDetalj>

@{
    ViewData["Title"] = "SelectWithDataSet";
}

<h1>SelectWithDataSet</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fornamn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Efternamn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Epost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fodelsear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Bor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fornamn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Efternamn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Epost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fodelsear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
        </tr>
}
    </tbody>
</table>

@if (ViewBag.antal != "")
{
    <p>Antal: @ViewBag.antal</p>
}
@if (ViewBag.error != "")
{
    <p>Error: @ViewBag.error</p>
}
